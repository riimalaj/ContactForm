datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Huolto {
  id           Int    @default(autoincrement()) @id
  createdAt    DateTime   @default(now())
  kohde        String
  huolto       String
  km           Int
  info         String
}


model Contact {
  id           Int    @default(autoincrement()) @id
  fName        String
  lName        String
  tel          String
  email        String
  other        String
}
