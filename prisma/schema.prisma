datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Playlist {
  id           Int    @default(autoincrement()) @id
  fName        String
  lName        String
  age          Int
}

model Maint {
  id           Int    @default(autoincrement()) @id
  kohde        String
  huolto        String
  km         Int
}